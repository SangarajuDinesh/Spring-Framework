<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="trainer" class="com.kn.Bean.Trainer">
		<property name="id" value="439"></property>
		<property name="name" value="Dinesh"></property>
		<property name="salary" value="35000"></property>
	</bean>
	<bean id="university" class="com.kn.Bean.University">
		<property name="id" value="111"></property>
		<property name="name" value="JNTUA"></property>
		<property name="trainer" ref="trainer"></property>
	</bean>
</beans>