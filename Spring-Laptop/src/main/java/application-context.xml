<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="s1" class="com.kn.main.Laptop">
		<property name="manufacturerId" value="111"></property>
		<property name="name" value="Dell"></property>
		<property name="cost" value="45000"></property>
	</bean>
	<bean id="s2" class="com.kn.main.Laptop">
		<property name="manufacturerId" value="222"></property>
		<property name="name" value="HP"></property>
		<property name="cost" value="50000"></property>
	</bean>
	<bean id="s3" class="com.kn.main.Laptop">
		<constructor-arg name="cost" value="45000"></constructor-arg>
		<constructor-arg name="manufacturerId" value="333"></constructor-arg>
		<constructor-arg name="name" value="Lenovo"></constructor-arg>
	</bean>
</beans>
